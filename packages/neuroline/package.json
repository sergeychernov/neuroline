{
	"name": "neuroline",
	"version": "0.9.1",
	"description": "Framework-agnostic pipeline orchestration with typed jobs and pluggable storage.",
	"keywords": [
		"pipeline",
		"orchestration",
		"workflow",
		"job",
		"typescript",
		"neuroline"
	],
	"homepage": "https://neuroline.vercel.app/",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sergeychernov/neuroline.git",
		"directory": "packages/neuroline"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		},
		"./mongo": {
			"types": "./dist/mongo.d.ts",
			"import": "./dist/mongo.mjs",
			"require": "./dist/mongo.js"
		},
		"./client": {
			"types": "./dist/client.d.ts",
			"import": "./dist/client.mjs",
			"require": "./dist/client.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup src/index.ts src/mongo.ts src/client.ts --format cjs,esm --dts --external mongoose --external react --external react-dom --external @mui/material --external @mui/icons-material --external @emotion/react --external @emotion/styled",
		"dev": "tsup src/index.ts src/mongo.ts src/client.ts --format cjs,esm --dts --watch --external mongoose --external react --external react-dom --external @mui/material --external @mui/icons-material --external @emotion/react --external @emotion/styled",
		"lint": "tsc --noEmit",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage"
	},
	"peerDependencies": {
		"@emotion/react": "^11.11.0",
		"@emotion/styled": "^11.11.0",
		"@mui/icons-material": "^5.15.0",
		"@mui/material": "^5.15.0",
		"mongoose": "^8.0.0",
		"react": "^18.2.0"
	},
	"peerDependenciesMeta": {
		"@emotion/react": {
			"optional": true
		},
		"@emotion/styled": {
			"optional": true
		},
		"@mui/icons-material": {
			"optional": true
		},
		"@mui/material": {
			"optional": true
		},
		"mongoose": {
			"optional": true
		},
		"react": {
			"optional": true
		}
	},
	"devDependencies": {
		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.1",
		"@mui/icons-material": "^5",
		"@mui/material": "^5",
		"@types/react": "^18",
		"@vitest/coverage-v8": "^4",
		"mongoose": "^8",
		"react": "^18",
		"tsup": "^8.5.1",
		"typescript": "^5.9.3",
		"vitest": "^4"
	}
}
