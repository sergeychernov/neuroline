{
  "name": "neuroline-ui",
  "version": "0.6.1",
  "description": "React UI components for visualizing Neuroline pipelines with MUI.",
  "homepage": "https://neuroline.vercel.app/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sergeychernov/neuroline.git",
    "directory": "packages/neuroline-ui"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --external react --external @mui/material --external @mui/icons-material --external @emotion/react --external @emotion/styled --external lodash",
    "dev": "tsup src/index.ts --format cjs,esm --dts --external react --external @mui/material --external @mui/icons-material --external @emotion/react --external @emotion/styled --external lodash --watch",
    "lint": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "lodash": "^4.17.23"
  },
  "peerDependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.0",
    "@mui/material": "^5.15.0",
    "neuroline": "^0.9.0",
    "react": "^18.2.0 || ^19.0.0",
    "react-dom": "^18.2.0 || ^19.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^5.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^5",
    "@mui/material": "^5",
    "@storybook/addon-docs": "^10.2.3",
    "@storybook/react-vite": "^10.2.3",
    "@types/lodash": "^4.17.23",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "neuroline": "workspace:*",
    "react": "^18",
    "react-dom": "^18",
    "storybook": "^10.2.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vite": "^7"
  }
}
