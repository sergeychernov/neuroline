{
	"name": "demo-pipelines",
	"version": "0.1.0",
	"private": true,
	"description": "Демо pipelines для тестирования neuroline",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"build": "tsup src/index.ts --format cjs,esm --dts --clean",
		"dev": "tsup src/index.ts --format cjs,esm --dts --watch",
		"lint": "tsc --noEmit"
	},
	"dependencies": {
		"neuroline": "workspace:*"
	},
	"devDependencies": {
		"tsup": "^8.0.1",
		"typescript": "^5.3.3"
	}
}
