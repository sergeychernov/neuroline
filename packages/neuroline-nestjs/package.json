{
  "name": "neuroline-nestjs",
  "version": "0.3.1",
  "homepage": "https://neuroline.vercel.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/sergeychernov/neuroline.git",
    "directory": "packages/neuroline-nestjs"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --external @nestjs/common --external @nestjs/core --external neuroline --external mongoose",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch --external @nestjs/common --external @nestjs/core --external neuroline --external mongoose",
    "lint": "tsc --noEmit"
  },
  "peerDependencies": {
    "@nestjs/common": ">=10.0.0",
    "@nestjs/core": ">=10.0.0",
    "neuroline": "workspace:*"
  },
  "devDependencies": {
    "@nestjs/common": "^10.3.0",
    "@nestjs/core": "^10.3.0",
    "@swc/core": "^1.15.7",
    "neuroline": "workspace:*",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}